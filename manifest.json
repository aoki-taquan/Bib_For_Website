{
    "manifest_version": 3,
    "name": "Bib For Website",
    "version": "1.0",
    "permissions": [
        "tabs"
    ],
    "action": {
        "default_title": "Bib For Website",
        "default_popup": "popup.html",
        "default_icon": {
            "32": "icons/32.png",
            "72": "icons/72.png",
            "128": "icons/128.png",
            "512": "icons/512.png"
        }
    },
    "icons": {
        "32": "icons/32.png",
        "72": "icons/72.png",
        "128": "icons/128.png",
        "512": "icons/512.png"
    }
}